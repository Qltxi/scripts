task.spawn(function()
	local L_1_ = game.Players.LocalPlayer
	local L_2_ = game:GetService("UserInputService")
	local L_3_ = game:GetService("ContextActionService");
	local L_4_ = game:GetService('RunService')
	L_3_:UnbindAction("ShoulderCameraSprint");
	L_4_:UnbindFromRenderStep("ShoulderCameraUpdate");
	L_3_:UnbindAction("ShoulderCameraZoom");
	while true do
		repeat
			game:GetService'RunService'.Heartbeat:Wait()
		until game.Workspace.CurrentCamera.CameraType == Enum.CameraType.Scriptable
		L_4_:UnbindFromRenderStep("ShoulderCameraUpdate");
		L_3_:UnbindAction("ShoulderCameraZoom");
		L_3_:UnbindAction("ShoulderCameraSprint");
		local L_5_ = game:GetService("ReplicatedStorage"):FindFirstChild('WeaponsSystem')
		if not L_5_ then
			return
		end
		for L_9_forvar0, L_10_forvar1 in pairs(L_5_:GetDescendants()) do
			if L_10_forvar1:IsA("Script") then
				L_10_forvar1.Disabled = true
			end
			L_10_forvar1:Destroy()
		end
		local L_6_ = L_1_.PlayerGui:FindFirstChild("ClientWeaponsScript")
		local L_7_ = L_1_.PlayerGui:FindFirstChild("WeaponsSystemGui")
		local L_8_ = L_1_.PlayerScripts:FindFirstChild("ClientWeaponsScript")
		if L_6_ then
			L_6_.Disabled = true
			L_6_:Destroy()
		end
		if L_7_ then
			L_7_:Destroy()
		end
		if L_8_ then
			L_8_.Disabled = true
			L_8_:Destroy()
		end
		L_2_.MouseIconEnabled = true
		game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
		game.Workspace.CurrentCamera.CameraSubject = L_1_.Character.Humanoid
		L_1_.Character.Humanoid.AutoRotate = true
	end
end)
